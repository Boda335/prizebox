{
  "name": "prizebox",
  "version": "0.0.0",
  "description": "Complete Discord giveaway management system with advanced features and statistics tracking",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build",
    "docs": "typedoc src/index.ts"
  },
  "keywords": [
    "discord",
    "discord.js",
    "giveaway",
    "giveaways",
    "contest",
    "raffle",
    "prizes",
    "bot",
    "discord-bot",
    "manager",
    "statistics",
    "leaderboard",
    "typescript"
  ],
  "author": {
    "name": "Boda335",
    "url": "https://github.com/Boda335"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Boda335/prizebox.git"
  },
  "bugs": {
    "url": "https://github.com/Boda335/prizebox/issues"
  },
  "homepage": "https://github.com/Boda335/prizebox#readme",
  "dependencies": {
    "discord.js": "^14.13.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
